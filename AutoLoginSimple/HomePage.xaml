<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="AutoLoginSimple.HomePage">
  <ContentPage.Content>
    <StackLayout Spacing="20" VerticalOptions="CenterAndExpand">
      <Label Text="Welcome!" x:Name="label_home" VerticalOptions="Center" HorizontalOptions="Center" />
      <StackLayout>
        <Button Text="Disconnect" x:Name="btn_logout"  Clicked="Disconnect_clicked"></Button>
      </StackLayout>
    </StackLayout>
  </ContentPage.Content>
 
</ContentPage>